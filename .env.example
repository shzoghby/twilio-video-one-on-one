# description: Choose a passcode for your app. Users have to use this passcode to enter the video call
# format: secret
# required: true
PASSCODE=2000

# description: current url of your twilio function. It will be used in the statuscallback of the room
# format: text
# required: true
WEB_URL=https://twilio-video-one-on-one-7316-dev.twil.io
